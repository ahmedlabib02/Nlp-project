    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "vscode": {
          "languageId": "powershell"
        }
      },
      "outputs": [],
      "source": [
        "def farasa_tokenize(text):\n",
        "    text = str(text)\n",
        "    segmented_text = segmenter.segment(text)\n",
        "    tokens = segmented_text.split()\n",
        "    return tokens\n",
        "\n",
        "df['farasa_tokens'] = df['transcript'].apply(farasa_tokenize)\n",
        "df['farasa_tokens'] "
      ]
